{
    "name": "styleci/git",
    "description": "StyleCI Git Is A Git Repository Manager",
    "keywords": ["git", "repo", "repository", "StyleCI", "StyleCI", "Git", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/filesystem": "~2.3",
        "gitonomy/gitlib": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\Git\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\Git\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
